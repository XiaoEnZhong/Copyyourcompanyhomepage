<script type="text/javascript" src="https://company.mangot5.com//resource/mall/js/jquery.js"></script>
<script type="text/javascript" src="https://company.mangot5.com//resource/js/jquery.twbsPagination.min.js"></script>
<script>
	//========================= 分页 페이지 매김   ==============================//
		var $pagination = $('.pagination');
		var currentPage = 1;
		var totalPages = 35;

		var informationObj = {
				setting: {
					  totalPages: totalPages,
					  visiblePages: 5,
					  hideOnlyOnePage: true,
					  first: '<span aria-hidden="true">&laquo;</span>',
					  last: '<span aria-hidden="true">&raquo;</span>',
					  prev: "",
					  next: "",
					  onPageClick: function (event, page) {
						  if(currentPage != page) informationObj.pagination(page);
						  currentPage = page;
					  }
				},
				pagination: function(page){
					$('body').addClass('waitingMouse');
					$pagination.twbsPagination('disable');
	//================= 點選分頁時，關閉點選功能，滑鼠鼠標更變為沙漏===============//
					$.post('https://company.mangot5.com//company/information',{page: page},
				       function(data, textStatus) {    	
				       $(".notice").html(data);
				       $pagination.twbsPagination('enable');
				       $('body').removeClass('waitingMouse');
				  }, "html");
				}
		}
		
		$pagination.twbsPagination(informationObj.setting);
		
</script><footer class="footer">
